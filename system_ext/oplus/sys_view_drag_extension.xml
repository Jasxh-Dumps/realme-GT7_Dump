<filter-conf>
    <!-- global config -->
    <version>20240823</version>
    <!-- 1:open 0:close -->
    <open>1</open>
    <filter-name>sys_view_drag_extension</filter-name>
    <!-- 1:Text 10:Image 100:WebView 1000:File View 10000:Normal View -->
    <drag-type>31</drag-type>
    <!-- 1:open 0:close -->
    <log>1</log>
    <!-- 1:open 0:close -->
    <data-tracking>1</data-tracking>
    <!-- ms -->
    <press-timeout>1100</press-timeout>
    <!-- px -->
    <touch-slop>24</touch-slop>
    <!-- 1\3\4\5\6\7\8  same with Bitmap.Config -->
    <bitmap-config>4</bitmap-config>
    <!-- 0~1, float -->
    <bitmap-scale>1</bitmap-scale>

    <!-- 微博 -->
    <package>
        <name>com.sina.weibo</name>
        <press-timeout>1100</press-timeout>
        <drag-type>3</drag-type>
        <log>1</log>
        <data-tracking>1</data-tracking>
        <!-- 聊天界面 -->
        <activity>
            <name>com.sina.weibo.weiyou.chat.single.DMSingleChatActivity</name>
            <drag-type>3</drag-type>
            <!-- 聊天文本、聊天图片 -->
            <target-views>RoundedImageView,DMTextView</target-views>
        </activity>
        <!-- 聊天大图界面 -->
        <activity>
            <name>com.sina.weibo.preview.MediaPreviewActivity</name>
            <drag-type>3</drag-type>
            <!-- 图片 -->
            <target-views>SplitDraggableImageView</target-views>
        </activity>
        <!-- 微博首页3个标签页 -->
        <activity>
            <name>com.sina.weibo.MainTabActivity</name>
            <drag-type>1</drag-type>
            <!-- 正文文本-->
            <target-views>MaxLinedLayoutTextView</target-views>
        </activity>
        <!-- 微博正文 -->
        <activity>
            <name>com.sina.weibo.feed.DetailWeiboActivity</name>
            <drag-type>19</drag-type>
            <!-- 正文文本、评论文本、正文静态图片、 -->
            <target-views>MBlogDetailTextView,MBlogTextView,ImageView,CommentPicView,BigImageView</target-views>
        </activity>
    </package>

    <!-- Bilibili ok -->
