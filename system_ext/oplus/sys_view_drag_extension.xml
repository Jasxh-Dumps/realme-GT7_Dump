        <!-- 图片放大界面 -->
        <activity>
            <name>com.bilibili.bplus.im.conversation.ImagesViewerActivity</name>
            <drag-type>3</drag-type>
            <!-- 图片 -->
            <target-views>PinchBiliImageView</target-views>
        </activity>
        <!-- 动态页面 -->
        <activity>
            <name>com.bilibili.lib.ui.ComposeActivity</name>
            <drag-type>3</drag-type>
            <!-- 图片 -->
            <target-views>OpusSpanTextView,ThemeBiliImageView,BiliImageView</target-views>
        </activity>
        <!-- 动态大图页面 -->
        <activity>
            <name>com.bilibili.bplus.followinglist.page.browser.ui.LightBrowserActivityV2</name>
            <drag-type>3</drag-type>
            <!-- 图片 -->
            <target-views>PinchImageView</target-views>
