    <version>20250212</version>

    <lowvoltage_brightness_limit support="1" name="P_3" temperatureRange="10,100">
        <limitnit batteryRange="0, 5C">1000</limitnit>  <!--batteryRange (a,b]-->
        <limitnit batteryRange="5, 15C">2273</limitnit>
        <limitnit batteryRange="15, 100C">2273</limitnit>
    </lowvoltage_brightness_limit>

